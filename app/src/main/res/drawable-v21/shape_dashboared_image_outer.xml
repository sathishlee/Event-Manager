<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="oval">

            <padding android:bottom="@dimen/dashboard_shape_img_strok_width"
                android:top="@dimen/dashboard_shape_img_strok_width"
                android:left="@dimen/dashboard_shape_img_strok_width"
                android:right="@dimen/dashboard_shape_img_strok_width"/>

            <solid
                android:color="@color/bg_dash_board_round_shape"/>

        </shape>
    </item>
<item>
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <corners
        android:radius="@dimen/dashboard_shape_img_radius"/>
    <gradient
        android:angle="90"
         />
    <solid
        android:color="@color/bg_dash_board_round_shape"/>
    <size
        android:width="@dimen/dashboard_shape_img_width"
        android:height="@dimen/dashboard_shape_img_height" />
    <stroke
        android:width="@dimen/dashboard_shape_img_strok_width"
        android:color="@color/colorPrimary" />
</shape>
</item>
</layer-list>